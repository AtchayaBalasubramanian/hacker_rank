import java.io.*;
import java.math.*;
import java.security.*;
import java.text.*;
import java.util.*;
import java.util.concurrent.*;
import java.util.regex.*;



public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
        int i;
        for(i=1;i<=10;i++)
        {
           int result=N*i;
           System.out.println(N+" x "+ i+" = "+result);
            
        }
    
    }
}
